{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": ["next", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
},
  "globals": {
    "stripe": true,
    "process": true
  }
}
